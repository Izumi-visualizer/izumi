OUTPUT_DIRECTORY        = @abs_builddir@/out

INPUT                   = @abs_srcdir@/..
STRIP_FROM_PATH         = @abs_srcdir@/../include
STRIP_FROM_INC_PATH     = @abs_srcdir@/../include
RECURSIVE               = YES

PROJECT_NAME            = libizumi
PROJECT_BRIEF           = A library of parsers for pipeline traces
OPTIMIZE_OUTPUT_FOR_C   = YES

JAVADOC_AUTOBRIEF       = YES

FULL_SIDEBAR            = YES
